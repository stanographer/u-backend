  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass --include-path ./src  --include-path ./node_modules src/ -o src/compiled",
    "build-js": "react-scripts build",
    "deploy": "cd ~/Developer/upwordly/client/build && scp -r . stanley@upword.ly:/var/www/upword.ly/html",
    "move-build-to-server": "mv build _build && mv _build ../server && cd ../server && rm -rf public && mv _build public",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "startdev": "NODE_PATH=./src REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
    "start-js": "NODE_PATH=./src/ react-scripts start",
    "clean": "rm -rf node_modules",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^5.6.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5"
  }
}
